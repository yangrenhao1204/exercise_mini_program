.header {
    position: fixed;
    top: 0;
    height: 100rpx;
    width: 100%;
    border-radius: 0 0 80% 80%;
}

.userinfo {
    display: flex;
    border-radius: 5rpx;
    flex-direction: column;
    align-items: center;
    padding-top: 100rpx;
}

.userinfo-avator-wrap {
    width: 134rpx;
    height: 134rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50%;
    background-color: #f8f7f7;
    top: 50px;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    top: 50px;
}

.userinfo-nickname {
    top: 50px;
}

.body {
    margin-top: 100rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
}

.item {
    line-height: 40px;
    padding-left: 10px;
    padding-right: 10px;
}

.type {
    padding-top: 50rpx;
    justify-content: center;
    align-content: center;
    text-align: center;
}

.image {
    height: 250rpx;
    width: 250rpx;
    padding: 4px 19px;
}

.navigator-hover {
    color: blue;
}
